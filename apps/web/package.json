{
  "name": "web",
  "description": "A meta app powered by TanStack Start",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vinxi dev --config ./app.config.ts",
    "build": "vinxi build --config ./app.config.ts",
    "start": "vinxi start -config ./app.config.ts",
    "lint": "prettier --check '**/*' --ignore-unknown && eslint --ext .ts,.tsx ./app",
    "format": "prettier --write '**/*' --ignore-unknown"
  },
  "dependencies": {
    "@tanstack/react-router": "1.74.4",
    "@tanstack/start": "1.74.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ui": "workspace:*",
    "vinxi": "^0.4.3"
  },
  "devDependencies": {
    "@types/node": "^20.12.11",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "10.4.16",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.3.3",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.4.14",
    "tsconfig": "workspace:*",
    "typescript": "^5.5.3",
    "vite": "^5.3.5",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
